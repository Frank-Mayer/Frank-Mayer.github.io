---
import Layout from "../layouts/Layout.astro"
import Recent from "../components/solid/Recent"
import { fetchRepos } from "../lib/repo"
const fetchedRepos = await fetchRepos()
---

<Layout>
    <h2>Recent</h2>
    <Recent client:load />
    <input
        type="hidden"
        id="prefetched-repos"
        value={JSON.stringify(fetchedRepos)}
    />
</Layout>
